<div class="container">
  <form method="post" action="/api/storeWarranty" enctype="multipart/form-data">
    <div class="mt-4 row">
      <div class="form-group col-md-6 my-4">
        <label for="email">Email</label>
        <input
          type="email"
          class="form-control"
          id="inputEmail4"
          placeholder="Email"
          name="email"
        />
      </div>
      <div class="form-group col-md-6 my-4">
        <label for="name">HoVaTen</label>
        <input
          type="text"
          class="form-control"
          placeholder="HoVaTen"
          name="name"
        />
      </div>
      <div class="form-group col-md-6 my-4">
        <label for="phone">Số Điện Thoại</label>
        <input
          type="text"
          class="form-control"
          placeholder="Nhập số điện thoại"
          name="phone"
        />
      </div>
      <div class="form-group col-md-6 my-4">
        <label for="address">Địa Chỉ</label>
        <input
          type="text"
          class="form-control"
          placeholder="Địa Chỉ"
          name="address"
        />
      </div>
    </div>

    <div class="form-group col-md-6 my-4">
      <label for="images">Chọn Ảnh</label>
      <input type="file" class="form-control-file" name="images" multiple />
    </div>
    <div class="form-group col-md-2 my-4">
      <label for="exampleFormControlFile1">Chọn Sản Phẩm</label>
      <input
        type="text"
        id="searchInput"
        list="suggestions"
        placeholder="Tìm kiếm"
      />
      <datalist id="suggestions">
          <option value="1"></option>
          <option value="2"></option>
          <option value="3"></option>
          <option value="4"></option>
    </div>
    <div class="form-group col-md-2 my-4">
      <button type="submit" class="btn btn-primary">Tạo</button>
    </div>
  </form>
</div>