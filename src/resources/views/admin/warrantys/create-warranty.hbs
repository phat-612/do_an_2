<div class="container">
  <form method="post" action="/api/storeWarranty">
    <div class="form-row mt-4">
      <div class="form-group col-md-6 p-4">
        <label for="inputEmail4">Email</label>
        <input
          type="email"
          class="form-control"
          id="inputEmail4"
          placeholder="Email"
        />
      </div>
      <div class="form-group col-md-6 p-4">
        <label for="inputPassword4">HoVaTen</label>
        <input type="" class="form-control" placeholder="HoVaTen" />
      </div>
    </div>
    <div class="form-group p-4">
      <label for="inputAddress">Số Điện Thoại</label>
      <input
        type="text"
        class="form-control"
        placeholder="Nhập số điện thoại"
      />
    </div>
    <div class="form-group p-4">
      <label for="inputAddress2">Địa Chỉ</label>
      <input type="text" class="form-control" placeholder="Địa Chỉ" />
    </div>
    <div class="form-group p-4">
      <label for="exampleFormControlFile1">Chọn Ảnh</label>
      <input
        type="file"
        class="form-control-file"
        id="exampleFormControlFile1"
      />
    </div>
    <div class="form-group col-md-2 p-4">
      <label for="exampleFormControlFile1">Chọn Sản Phẩm</label>
      <input
        type="text"
        id="searchInput"
        list="suggestions"
        placeholder="Tìm kiếm"
      />
      <datalist id="suggestions">
        <option value="Apple"></option>
        <option value="Banana"></option>
        <option value="Orange"></option>
        <option value="Mango"></option>
      </datalist>
    </div>
    <button type="submit" class="btn btn-primary">Tạo</button>
  </form>
</div>