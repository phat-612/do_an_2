<div class="container">
  <form method="post" action="/api/storeWarranty" enctype="multipart/form-data">
    <div class="mt-4 row">
      <div class="form-group col-md-6 my-4">
        <label for="email">Email</label>
        <input
          type="email"
          class="form-control"
          id="email"
          placeholder="Email"
          value="{{warrantys.email}}"
          name="email"
        />
      </div>
      <div class="form-group col-md-6 my-4">
        <label for="name">HoVaTen</label>
        <input
          type="text"
          class="form-control"
          placeholder="HoVaTen"
          name="name"
          id="name"
          value="{{warrantys.name}}"
        />
      </div>
      <div class="form-group col-md-6 my-4">
        <label for="phone">Số Điện Thoại</label>
        <input
          type="text"
          class="form-control"
          placeholder="Nhập số điện thoại"
          name="phone"
          id="phone"
          value="{{warrantys.phone}}"   
        />
      </div>
      <div class="form-group col-md-6 my-4">
        <label for="address">Địa Chỉ</label>
        <input
          type="text"
          class="form-control"
          placeholder="Địa Chỉ"
          name="address"
          id="address"
          value="{{warrantys.address}}"

        />
      </div>
      <div class="form-group col-md-6 my-4">
        <label for="note">Ghi chú</label>
        <input
          type="text"
          class="form-control"
          placeholder="Nhập ghi chú"
          id="note"
          name="note"  
          value="{{warrantys.note}}"

        />
      </div>
      <div class="form-group col-md-6 my-4">
        <label for="status">Trạng thái</label>
        <input
          type="text"
          class="form-control"
           placeholder="Chọn trạng thái"
          id="status"
          name="status"
          value="{{warrantys.status}}"
        />
      </div>
      <div class="form-group col-md-6 my-4">
        <label for="total">Gia</label>
        <input
          type="text"
          class="form-control"
          placeholder="Nhập giá"
          id="total"
          name="total"
          value="{{warrantys.total}}"
        />
      </div>
    </div>
    <div class="form-group col-md-6 my-4">
      <label for="images">Chọn Ảnh</label>
      <input type="file" class="form-control-file" value="{{warranty.images}}" id="images" name="images" multiple />
    </div>
    <div class="form-group col-md-2 my-4">
      <label for="exampleFormCon
      trolFile1">Chọn Sản Phẩm</label>
      <input
        type="text"
        id="searchInput"
        list="suggestions"
        placeholder="Tìm kiếm"
      />
    <datalist id="suggestions">
      {{#each products}}
      <option value="{{this.name}}"></option>
      {{/each}}
    </datalist>
    <div class="form-group  my-4">
      <button type="submit" class="btn btn-primary">Lưu chỉnh sửa</button>
    </div>
  </form>
</div>
