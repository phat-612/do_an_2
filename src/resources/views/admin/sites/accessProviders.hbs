<div class="container">
  <div class="nav d-flex justify-content-end my-3">
    <form class="d-flex" id="searchForm">
      <input
        class="form-control me-2"
        type="search"
        placeholder="Search"
        aria-label="Search"
        id="searchInput"
      />
      <button class="btn btn-outline-success" type="submit">Search</button>
    </form>
  </div>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Email</th>
        <th scope="col">Họ Tên</th>
        <th scope="col" class="col-3">Hành Động</th>
      </tr>
    </thead>
    <tbody>
      {{#each users}}
        <tr>
          <td>{{this.email}}</td>
          <td>{{this.name}}</td>
          <td>
            <select class="form-select" aria-label="Default select example">
              
              <option value="user" {{#if (compare this.role "user")}}selected{{/if}} >Khách Hàng</option>
              <option value="admin" {{#if (compare this.role "admin")}}selected{{/if}} >Admin</option>
              
            </select>
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>